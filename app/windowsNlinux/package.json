{
  "name": "geniemoji",
  "version": "5.0.0",
  "description": "The Emoji Genie",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-win32-x64": "electron-packager . Geniemoji --platform=win32 --arch=x64 --icon=assets/AppIcons/icon.ico --ignore=builds --ignore=docs --ignore=assets/PreviewImg --ignore=assets/PreviewVid --ignore=assets/oldIcons",
    "pack-linux-x64": "electron-packager . Geniemoji --platform=linux --arch=x64 --icon=assets/AppIcons/icon.png --ignore=builds --ignore=docs --ignore=assets/PreviewImg --ignore=assets/PreviewVid --ignore=assets/oldIcons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virejdasani/Geniemoji.git"
  },
  "keywords": [
    "Geniemoji",
    "Emoji",
    "Genie",
    "Emogenie",
    "Emojenie"
  ],
  "author": "Virej Dasani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/virejdasani/Geniemoji/issues"
  },
  "homepage": "https://github.com/virejdasani/Geniemoji#readme",
  "dependencies": {
    "electron": "^12.0.4",
    "open": "^8.0.9"
  }
}
